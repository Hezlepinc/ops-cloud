  "content": "<!--\r\n  Hezlep Inc - Services Grid Pattern\r\n-->\r\n\r\n<!-- wp:group {\"className\":\"services-grid section-surface\",\"layout\":{\"type\":\"constrained\"}} -->\r\n  <!-- wp:heading {\"level\":2} -->\r\n  <h2>Capabilities</h2>\r\n  <!-- /wp:heading -->\r\n\r\n  <!-- wp:columns {\"columns\":3} -->\r\n    <!-- wp:column -->\r\n      <!-- wp:group {\"className\":\"oc-card\",\"layout\":{\"type\":\"constrained\"}} -->\r\n        <!-- wp:heading {\"level\":3} -->\r\n        <h3>Automation</h3>\r\n        <!-- /wp:heading -->\r\n        <!-- wp:paragraph -->\r\n        <p>Zapier/Make, CRM workflows, AI assistance.</p>\r\n        <!-- /wp:paragraph -->\r\n      <!-- /wp:group -->\r\n    <!-- /wp:column -->\r\n\r\n    <!-- wp:column -->\r\n      <!-- wp:group {\"className\":\"oc-card\",\"layout\":{\"type\":\"constrained\"}} -->\r\n        <!-- wp:heading {\"level\":3} -->\r\n        <h3>Web Ops</h3>\r\n        <!-- /wp:heading -->\r\n        <!-- wp:paragraph -->\r\n        <p>WordPress, hosting, CI/CD, performance.</p>\r\n        <!-- /wp:paragraph -->\r\n      <!-- /wp:group -->\r\n    <!-- /wp:column -->\r\n\r\n    <!-- wp:column -->\r\n      <!-- wp:group {\"className\":\"oc-card\",\"layout\":{\"type\":\"constrained\"}} -->\r\n        <!-- wp:heading {\"level\":3} -->\r\n        <h3>Growth Systems</h3>\r\n        <!-- /wp:heading -->\r\n        <!-- wp:paragraph -->\r\n        <p>Analytics, SEO, lead pipelines, reporting.</p>\r\n        <!-- /wp:paragraph -->\r\n      <!-- /wp:group -->\r\n    <!-- /wp:column -->\r\n  <!-- /wp:columns -->\r\n<!-- /wp:group -->\n\n<!--\r\n  Hezlep Inc - Proof Snapshot Pattern\r\n-->\r\n\r\n<!-- wp:group {\"className\":\"section-surface proof-snapshot\",\"layout\":{\"type\":\"constrained\"}} -->\r\n  <!-- wp:heading {\"level\":2} -->\r\n  <h2>Proof that the system works</h2>\r\n  <!-- /wp:heading -->\r\n\r\n  <!-- wp:paragraph -->\r\n  <p>We partner with builders, agencies, and operators who want compounding leverage — not more busywork.</p>\r\n  <!-- /wp:paragraph -->\r\n\r\n  <!-- wp:columns {\"columns\":3} -->\r\n    <!-- wp:column -->\r\n      <!-- wp:group {\"className\":\"oc-card oc-metric\",\"layout\":{\"type\":\"constrained\"}} -->\r\n        <!-- wp:heading {\"level\":3} -->\r\n        <h3>3–5x</h3>\r\n        <!-- /wp:heading -->\r\n        <!-- wp:paragraph -->\r\n        <p>Increase in usable ops visibility, from scattered spreadsheets to a single ops view.</p>\r\n        <!-- /wp:paragraph -->\r\n      <!-- /wp:group -->\r\n    <!-- /wp:column -->\r\n\r\n    <!-- wp:column -->\r\n      <!-- wp:group {\"className\":\"oc-card oc-metric\",\"layout\":{\"type\":\"constrained\"}} -->\r\n        <!-- wp:heading {\"level\":3} -->\r\n        <h3>30–60 days</h3>\r\n        <!-- /wp:heading -->\r\n        <!-- wp:paragraph -->\r\n        <p>Typical time from audit to first workflow live in production.</p>\r\n        <!-- /wp:paragraph -->\r\n      <!-- /wp:group -->\r\n    <!-- /wp:column -->\r\n\r\n    <!-- wp:column -->\r\n      <!-- wp:group {\"className\":\"oc-card oc-metric\",\"layout\":{\"type\":\"constrained\"}} -->\r\n        <!-- wp:heading {\"level\":3} -->\r\n        <h3>1 team</h3>\r\n        <!-- /wp:heading -->\r\n        <!-- wp:paragraph -->\r\n        <p>Strategy, implementation, and enablement under one roof — no handoff gaps.</p>\r\n        <!-- /wp:paragraph -->\r\n      <!-- /wp:group -->\r\n    <!-- /wp:column -->\r\n  <!-- /wp:columns -->\r\n<!-- /wp:group -->\n\n<!--\r\n  Hezlep Inc - CTA Banner Pattern\r\n-->\r\n\r\n<!-- wp:group {\"className\":\"cta-banner\",\"layout\":{\"type\":\"constrained\"}} -->\r\n  <!-- wp:group {\"className\":\"oc-card\",\"layout\":{\"type\":\"constrained\"}} -->\r\n    <!-- wp:heading {\"level\":2} -->\r\n    <h2>Let’s design your ops system.</h2>\r\n    <!-- /wp:heading -->\r\n    <!-- wp:paragraph -->\r\n    <p>Book a working session to see where your systems are holding you back—and what’s possible.</p>\r\n    <!-- /wp:paragraph -->\r\n    <!-- wp:buttons -->\r\n      <!-- wp:button {\"className\":\"btn-primary\"} -->\r\n        <a class=\"wp-block-button__link btn-primary\" href=\"/contact\">Start a project</a>\r\n      <!-- /wp:button -->\r\n    <!-- /wp:buttons -->\r\n  <!-- /wp:group -->\r\n<!-- /wp:group -->",
